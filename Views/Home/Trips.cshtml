@model User
<!doctype html>
<html lang="en">
  <head>
    <!-- Required meta tags -->
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">

    <!-- Bootstrap CSS -->
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.1.3/css/bootstrap.min.css" integrity="sha384-MCw98/SFnGE8fJT3GXwEOngsV7Zt27NXFoaoApmYm81iuXoPkFOJwJ8ERdknLPMO" crossorigin="anonymous">

    <title>The Place With the Stuff</title>
  </head>
  <body>
      <div class="container">

          <h1>Welcome, @ViewBag.name</h1>
          <a style="float: right;" href="/">Log Out</a>

          <table class="table">
              <thead>
                <tr>
                    <th>Destination</th>
                    <th>Date Start</th>
                    <th>Date End</th>
                    <th>Description</th>
                    <th>Action</th>
                </tr>
                @foreach(var i in @ViewBag.MyTrips){
                    <tr>
                        <th>@i.Trip.destination</th>
                        <th>@i.Trip.from</th>
                        <th>@i.Trip.to</th>
                        <th>@i.Trip.description</th>
                        <th>DELETE</th>
                    </tr>
                }
                @foreach(var i in @ViewBag.MyAddedTrips){
                    <tr>
                        <th>@i.Trip.destination</th>
                        <th>@i.Trip.from</th>
                        <th>@i.Trip.to</th>
                        <th>@i.Trip.description</th>
                        <th>DELETE</th>
                    </tr>
                }
                  
              </thead>
          </table>
          <table class="table">
                <thead>
                    <tr>
                        <th>Name</th>
                        <th>Destination</th>
                        <th>Date Start</th>
                        <th>Date End</th>
                        <th>Description</th>
                        <th>Action</th>
                    </tr>
                    @foreach(var i in @ViewBag.notMyTrips){
                      <th>@i.User.name</th>
                      <th>@i.Trip.destination</th>
                      <th>@i.Trip.from</th>
                      <th>@i.Trip.to</th>
                      <th>@i.Trip.description</th>
                      <th>Join</th>
                    }
                    
                </thead>
            </table>
          <a style="float: right;display: block;" href="/makeTrip">Create Trip</a>

      </div>


    <!-- Optional JavaScript -->
    <!-- jQuery first, then Popper.js, then Bootstrap JS -->
    <script src="https://code.jquery.com/jquery-3.3.1.slim.min.js" integrity="sha384-q8i/X+965DzO0rT7abK41JStQIAqVgRVzpbzo5smXKp4YfRvH+8abtTE1Pi6jizo" crossorigin="anonymous"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.14.3/umd/popper.min.js" integrity="sha384-ZMP7rVo3mIykV+2+9J3UJ46jBk0WLaUAdn689aCwoqbBJiSnjAK/l8WvCWPIPm49" crossorigin="anonymous"></script>
    <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.1.3/js/bootstrap.min.js" integrity="sha384-ChfqqxuZUCnJSK3+MXmPNIyE6ZbWh2IMqE241rYiqJxyMiZ6OW/JmZQ5stwEULTy" crossorigin="anonymous"></script>
  </body>
</html>